{
  "name": "newnewportfolio",
  "version": "0.2.0",
  "description": "Portfolio of Kevin Ports",
  "private": true,
  "browserslist": [
    "> 1%",
    "last 3 versions"
  ],
  "scripts": {
    "start": "NODE_ENV=development nodemon -e js,hbs index.js",
    "prod": "NODE_ENV=production node index.js",
    "rev": "rm -rf server/assets_rev/ && nodeRev 'server/assets/**/*.+(css|js|jpg|png|mp4|gif|webm)' --hash -o 'server/assets_rev/' --file 'server/asset.manifest.json'",
    "deploy": "node ./scripts/s3-sync"
  },
  "author": "Kevin Ports",
  "dependencies": {
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "hbs": "^4.0.1",
    "nodemon": "^1.19.1"
  },
  "devDependencies": {
    "aws-sdk": "^2.201.0",
    "autoprefixer": "^7.2.5",
    "node-rev": "^1.3.0",
    "s3": "^4.4.0",
    "shortid": "^2.2.8"
  }
}
